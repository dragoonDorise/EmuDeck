      "profileType": "template"
    },
    "ps4": null,
    "ps5": null,
    "xbox360": null,
    "xboxone": null,
    "switch_joycon_left": null,
    "switch_joycon_right": null
  },
  "parserId": "164770823452964732",
  "version": 15,
  "imageProviderAPIs": {
    "SteamGridDB": {
      "nsfw": false,
      "humor": false,
      "imageMotionTypes": ["static"],
      "styles": [],
      "stylesHero": [],
      "stylesLogo": [],
      "stylesIcon": []
    }
  },
  "defaultImage": {
    "long": "",
    "tall": "",
