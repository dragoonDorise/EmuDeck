<?xml version="1.0"?>
<systemList>
    <system>
        <name>wiiu</name>
        <fullname>Nintendo Wii U</fullname>
        <path>%ROMPATH%/wiiu</path>
        <extension>.rpx .RPX .wud .WUD .wux .WUX .elf .ELF .iso .ISO .wad .WAD</extension>
        <command label="Cemu (Proton)">/run/media/mmcblk0p1/Emulation/roms/wiiu/cemu.sh -f -g %ROM%</command>
        <platform>wiiu</platform>
        <theme>wiiu</theme>
    </system>
</systemList>
