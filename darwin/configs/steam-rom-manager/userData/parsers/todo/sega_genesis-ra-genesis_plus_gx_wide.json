  "defaultImage": "",
  "defaultTallImage": "",
  "defaultHeroImage": "",
  "defaultLogoImage": "",
  "defaultIcon": "",
  "localImages": "",
  "localTallImages": "",
  "localHeroImages": "",
  "localLogoImages": "",
  "localIcons": "",
